{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install | chmod ug+x .husky/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:run": "ts-node src/database/seeds/index.ts",
    "migrate:create": "migrate create --template-file ./src/database/utils/template.ts --migrations-dir=\"./src/database/migrations\" --compiler=\"ts:./src/database/utils/ts-compiler.js\"",
    "migrate:up": "migrate --migrations-dir=\"./src/database/migrations\" --compiler=\"ts:./src/database/utils/ts-compiler.js\" up",
    "migrate:down": "migrate --migrations-dir=\"./src/database/migrations\" --compiler=\"ts:./src/database/utils/ts-compiler.js\" down",
    "migrate:clear": "migrate --migrations-dir=\"./src/database/migrations\" --compiler=\"ts:./src/database/utils/ts-compiler.js\" clear",
    "migrate:list": "migrate --migrations-dir=\"./src/database/migrations\" --compiler=\"ts:./src/database/utils/ts-compiler.js\" list"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.462.0",
    "@aws-sdk/s3-request-presigner": "^3.462.0",
    "@esm2cjs/p-map": "^5.5.0",
    "@joi/date": "^2.1.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.16",
    "@types/bcrypt": "^5.0.0",
    "@types/hbs": "^4.0.2",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.3",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "google-auth-library": "^8.9.0",
    "googleapis": "^128.0.0",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "helmet": "^7.0.0",
    "husky": "^8.0.3",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "migrate": "^2.0.0",
    "mongoose": "^7.5.3",
    "mongoose-delete": "^1.0.1",
    "nest-winston": "^1.9.4",
    "nestjs-i18n": "^10.3.5",
    "nodemailer": "^6.9.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/mongoose-delete": "^1.0.2",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.11",
    "@types/supertest": "^2.0.13",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cache-manager-redis-yet": "^4.1.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
